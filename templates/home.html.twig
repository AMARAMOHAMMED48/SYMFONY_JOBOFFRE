
{% extends 'base.html.twig' %}
{% block title %}Hello DefaultController!{% endblock %}
{% block body %}
    <section class="hero-area style3">
     <table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">title</th>
      <th scope="col">body</th>
    
    </tr>
  </thead>
  <tbody>
  {% for task in tasks %}
